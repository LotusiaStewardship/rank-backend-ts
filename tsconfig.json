{
  "exclude": [
    "submodules/nanomsg",
    "node_modules"
  ],
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./.output",
    "declaration": false,
    "removeComments": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strict": false,
    "module": "Node16",
    "moduleResolution": "node16",
    "target": "ES2022",
    "lib": [
      "ES2022", "ES5", "ES6"
    ],
  }
}